    "import torch\n",
    "\n",
    "w = torch.tensor([[1.0, 5.0], [2.0, 6.0]], requires_grad=True)\n",
    "x = torch.tensor([[3.0, 7.0], [4.0, 8.0]], requires_grad=True)\n",
    "a = torch.add(w, x)\n",
    "b = torch.add(w, 1)\n",
    "y = torch.mul(a, b)\n",
    "print(a)\n",
    "print(b)\n",
    "print(y)\n",
    "y.sum().backward()\n",
    "print(w.grad)"

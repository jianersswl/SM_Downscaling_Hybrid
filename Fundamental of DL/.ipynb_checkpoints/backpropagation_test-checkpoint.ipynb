   "source": [
    "import torch\n",
    "\n",
    "w = torch.tensor([1., 2.], requires_grad=True)\n",
    "x = torch.tensor([3., 4.], requires_grad=True)\n",
    "print(w, x)\n",
    "a = torch.add(w, x)\n",
    "b = torch.add(w, 1)\n",
    "y = torch.mul(a, b)\n",
    "\n",
    "y.backward()\n",
    "print(w.grad)"
   ]
